package exam13;

public class dounleArraysExam01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//선언1
		int [][] arr= new int[3][5];
		//2
		int[][] arr1 = new int [3][];
		//3
		int[][] arr2 = { {1,2,3},{4,5,6}};
		
		//출력
		
		System.out.println(arr[1]);
		System.out.println(arr1[2]);
		System.out.println(arr2[1][1]);
	}

}
